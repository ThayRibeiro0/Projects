// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id        Int          @id @default(autoincrement())
  uuid      String       @unique @default(uuid())
  email     String       @unique
  password  String       
}